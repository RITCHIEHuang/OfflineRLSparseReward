{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--algo_name=mopo","--task=antmaze-medium-play-v0","--delay_mode=none","--strategy=none"],
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "cwd": "${fileDirname}"
        }
    ]
}